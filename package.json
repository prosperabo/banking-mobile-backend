{
  "name": "banking-mobile-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "orm:init": "npx prisma init",
    "orm:generate": "npx prisma generate",
    "orm:migrate:dev": "npx prisma migrate dev",
    "orm:migrate:deploy": "npx prisma migrate deploy",
    "prepare": "husky"
  },
  "keywords": [
    "banking",
    "mobile",
    "backend",
    "typescript",
    "express",
    "prisma"
  ],
  "author": "Prospera Servicios Financieros",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com-personal:prosperabo/banking-mobile-backend.git"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "@prisma/client": "^6.5.0",
    "@types/axios": "^0.9.36",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "resend": "^4.2.0",
    "source-map-support": "^0.5.21",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/form-data": "^2.2.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  },
  "_moduleAliases": {
    "@": "dist/src"
  }
